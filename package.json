{
  "name": "virtual_credit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "predev": "prisma generate",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "prebuild": "prisma generate",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "next lint --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "format:check": "eslint-config-prettier 'src/**/*.{js,jsx,ts,tsx}'",
    "prisma:migrate": "pnpm with-env prisma migrate dev --name init",
    "prisma:studio": "pnpm with-env prisma studio",
    "with-env": "dotenv -e .env.development.local --"
  },
  "dependencies": {
    "@clerk/localizations": "^1.22.1",
    "@clerk/nextjs": "^4.21.14",
    "@kuma-ui/core": "^1.0.1",
    "@prisma/client": "5.3.1",
    "@supabase/supabase-js": "^2.36.0",
    "@types/node": "20.4.0",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@upstash/redis": "^1.22.0",
    "@vercel/postgres": "^0.4.0",
    "clsx": "^1.2.1",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "framer-motion": "^10.12.18",
    "next": "13.4.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "svix": "^1.5.2",
    "typescript": "5.1.6",
    "undici": "^5.22.1"
  },
  "devDependencies": {
    "@kuma-ui/next-plugin": "^1.0.3",
    "@types/gtag.js": "^0.0.12",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "dotenv-cli": "^7.2.1",
    "encoding": "^0.1.13",
    "esbuild-register": "^3.4.2",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^3.0.0",
    "prisma": "^5.3.1"
  },
  "volta": {
    "node": "18.16.0"
  }
