{
  "name": "virtual_credit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && NODE_OPTIONS='--inspect' next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "next lint --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "format:check": "eslint-config-prettier 'src/**/*.{js,jsx,ts,tsx}'",
    "psconnect": "pscale connect virtual-credit-db dev --port 3309",
    "psdeploy": "pscale deploy-request create virtual-credit-db dev"
  },
  "dependencies": {
    "@clerk/localizations": "^1.17.1",
    "@clerk/nextjs": "^4.19.1",
    "@prisma/client": "^4.13.0",
    "@types/node": "20.1.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@vercel/postgres": "^0.2.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.1",
    "next": "13.4.1",
    "prisma": "^4.13.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "svix": "^1.4.12",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.59.9",
    "autoprefixer": "^10.4.14",
    "eslint-config-prettier": "^8.8.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2"
  }
}
